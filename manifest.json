{
  "name": "Calculator", //the primary identifier
  "description": "Build an Extension!", //
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["cal.js"]
    }
  ],
  "browser_action": {
    "default_icon":"calc.png",
     "default_popup": "cal.html",
    "default_title": "Calculator"
  }
}
